        ]
      },
      "inputs": {
        "type": "object",
        "properties": {
          "FreeSurferLicense": {
            "type": "object",
            "properties": {}
          },
          "GradientCoeff": {
            "type": "object",
            "properties": {}
          },
          "SiemensGREMagnitude": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "nifti"
                ]
              }
            }
          },
          "SiemensGREPhase": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "nifti"
                ]
              }
            }
          },
          "SpinEchoNegative": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "nifti"
                ]
              }
            }
          },
          "SpinEchoPositive": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "nifti"
                ]
              }
            }
          },
          "T1": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "nifti"
                ]
              }
            }
          },
          "T2": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "nifti"
                ]
              }
            }
          },
          "api-key": {
            "type": "object"
          }
        },
        "required": [
          "T1",
          "T2",
          "api-key"
        ]
      }
    },
    "required": [
      "config",
      "inputs"
    ]
  }
}